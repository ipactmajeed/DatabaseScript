MAX(CASE WHEN (T.DCALPHA1=7 AND T.DCALPHA2=1) THEN T.DCALPHA8 ELSE '''''''' END) LoanRemarks ''+@Select+''
FROM INV_DOCDETAILS I WITH(NOLOCK)
JOIN COM_DOCCCDATA C  WITH(NOLOCK) ON C.INVDOCDETAILSID=I.INVDOCDETAILSID
