JOIN COM_CCCCData CC WITH(NOLOCK) ON CC.NodeID=C51.NodeID AND CC.CostCenterID=50051
JOIN PAY_EMPPAY AP WITH(NOLOCK) ON AP.EmployeeID=C51.NodeID''+@strCCJoin+''
